
public class Palindron {
	public static void main(String[] args) {
		int no=123456;
		int rem=0,temp=0;
		while(no > 0) {
			rem = no%10;
			temp = temp*10 +rem;
			System.out.println(temp);
			no = no/10;
			int count = 0;
			count ++;
			System.out.println(count);

	
		}
		System.out.println(temp);
	}

}
